  "settings": ["theme_tab_selected_underlined", "theme_app_bar", "theme_tab_selected_transparent"],
  "attributes": ["selected"],
  "layer3.opacity": 1.0,
  "layer3.texture": "Boxy Theme/assets/common/tabs/tab_selected_underlined_white.png"
},

