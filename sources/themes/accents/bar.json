<% for (var i = 0; i < ui.accents.length; i++) { %>
// Bar <%= ui.accents[i].name %> Accent
// ===========================================================================

// Grid Border Color
// ---------------------------------------------------------------------------

{
  "class": "grid_layout_control",
  "settings": ["theme_app_bar"<% if (ui.accents[i].id != "blue") { %>, "theme_ui_accent_<%= ui.accents[i].id %>"<% } %>],
  "border_color": [<%= ui.accents[i].highlight.rgb %>]
},

// Tabset
// ---------------------------------------------------------------------------

{
  "class": "tabset_control",
  "settings": ["theme_app_bar"<% if (ui.accents[i].id != "blue") { %>, "theme_ui_accent_<%= ui.accents[i].id %>"<% } %>],
  "layer0.tint": [<%= ui.accents[i].rgb %>]
},

// Tab
// ---------------------------------------------------------------------------

{
  "class": "tab_control",
  "settings": ["theme_app_bar"<% if (ui.accents[i].id != "blue") { %>, "theme_ui_accent_<%= ui.accents[i].id %>"<% } %>],
  "layer0.texture": "Boxy Theme/assets/accents/<%= ui.accents[i].id %>/tabs/tab_selected_filled.png",
  "layer1.tint": [<%= ui.accents[i].highlight.rgb %>]
},

{
  "class": "tab_control",
  "settings": ["theme_app_bar"<% if (ui.accents[i].id != "blue") { %>, "theme_ui_accent_<%= ui.accents[i].id %>"<% } %>],
  "attributes": ["hover"],
  "layer1.opacity": 1.0
},

{
  "class": "tab_control",
  "settings": ["theme_tab_selected_transparent", "theme_app_bar"],
  "attributes": ["selected"],
  "layer1.opacity": 0
},

{
  "class": "tab_label",
  "parents": [{"class": "tab_control", "attributes": ["selected"]}],
  "settings": ["theme_app_bar", "theme_tab_selected_filled"<% if (ui.accents[i].id != "blue") { %>, "theme_ui_accent_<%= ui.accents[i].id %>"<% } %>],
  "fg": [<%= ui.base.white.rgb %>, 255]
},

{
  "class": "tab_close_button",
  "parents": [{"class": "tab_control", "attributes": ["selected"]}],
  "settings": ["theme_app_bar", "theme_tab_selected_filled"<% if (ui.accents[i].id != "blue") { %>, "theme_ui_accent_<%= ui.accents[i].id %>"<% } %>],
  "layer0.texture": "Boxy Theme/assets/variants/dark/buttons/button_close.png"
},

{
  "class": "tab_close_button",
  "parents": [{"class": "tab_control", "attributes": ["selected"]}],
  "settings": ["theme_app_bar", "theme_tab_selected_filled"<% if (ui.accents[i].id != "blue") { %>, "theme_ui_accent_<%= ui.accents[i].id %>"<% } %>],
  "attributes": ["hover"],
  "layer0.opacity": 1.0
},

{
  "class": "tab_close_button",
  "parents": [{"class": "tab_control", "attributes": ["selected"]}],
  "settings": ["theme_app_bar", "theme_tab_selected_transparent"<% if (ui.accents[i].id != "blue") { %>, "theme_ui_accent_<%= ui.accents[i].id %>"<% } %>],
  "layer0.texture": "Boxy Theme/assets/variants/dark/buttons/button_close.png"
},

{
  "class": "tab_close_button",
